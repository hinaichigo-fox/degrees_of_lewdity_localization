  {
    "passage": "Forest Shop Widgets",
    "filepath": "base-clothing/shop.twee",
    "key": "15_5_3_7|",
    "original": "It's Gwylan.",
    "translation": "",
    "pos": 347
  },
  {
    "passage": "Forest Shop Widgets",
    "filepath": "base-clothing/shop.twee",
    "key": "17_5_3_7|",
    "original": "\"You came!\"",
